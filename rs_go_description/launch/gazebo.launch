<launch>
  <arg name="world" default="world1"/>
  <param command="$(find xacro)/xacro $(find rs_go_description)/urdf/rs_go.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model rs_go" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find rs_go_description)/worlds/$(arg world).world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>
